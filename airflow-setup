mkdir airflow
cd airflow
curl -LfO 'https://airflow.apache.org/docs/apache-airflow/2.9.2/docker-compose.yaml' > docker-compose.yaml
sed -i '' 's/^[[:space:]]*# AIRFLOW_CONFIG/    AIRFLOW_CONFIG/' docker-compose.yaml
mkdir -p ./dags ./logs ./plugins ./config
cd config && curl -LfO 'https://github.com/puckel/docker-airflow/raw/master/config/airflow.cfg' > airflow.cfg && cd ..
echo "AIRFLOW_UID=$(id -u)" > .env
docker compose up airflow-init
docker compose up
